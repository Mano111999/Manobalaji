package com.balaji;

import java.util.Scanner;

public class ascii {
    public static void main(String[] args) {
        // ascii value
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter the Char:");
        char ch= sc.next().charAt(0);
        int ascii= ch;
        System.out.println("ASCII value :"+ ascii);
    }
}
